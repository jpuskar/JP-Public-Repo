  <TypeDefinitions>
    <EntityTypes>
      <ClassTypes>
        <ClassType ID="JPPacks.MonitorSyslogs.SyslogReceiver" Accessibility="Internal" Abstract="false" Base="Windows!Microsoft.Windows.ComputerRole" Hosted="true" Singleton="false" />
